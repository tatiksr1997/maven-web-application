node
{
    
 def mavenHome = tool name: "maven3.8.2"
 
 
 stage('CheckoutCode')
 {
   git branch: 'development', credentialsId: 'b78ee6ac-5609-4d88-bb10-eb0c4a086637', url: 'https://github.com/tatiksr1997/maven-web-application.git'
 }
 
 stage('Build')
 {
  sh "${mavenHome}/bin/mvn clean package"
 }
}
